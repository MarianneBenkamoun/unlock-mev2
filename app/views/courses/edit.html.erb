<% if (@course.photodoor.to_s == "" || @course.photoserrure.to_s == "") %>
  <%= simple_form_for(@course) do |f| %>
    <%= f.input :photodoor, input_html: { id: "photodoor"} %>
    <%= f.input :photodoor_cache, as: :hidden %>
    <%= f.input :photoserrure, input_html: { id: "photoserrure"}%>
    <%= f.input :photoserrure_cache, as: :hidden %>
    <%= f.button :submit, id:"myBtn" %>
  <% end %>
<% end%>

